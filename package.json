{
    "name": "api-errands",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only --cls ./src/index.ts",
        "dev:debug": "ts-node-dev --inspect --respawn --cls --transpile-only ./src/index.ts",
        "postinstall": "tsc",
        "start": "node ./build/index.js",
        "build": "tsc",
        "typeorm": "typeorm-ts-node-commonjs",
        "typeorm:create": "typeorm migration:create",
        "typeorm:run": "npm run typeorm migration:run -- -d ./src/database/config/database.config.ts",
        "typeorm:revert": "npm run typeorm migration:revert -- -d ./src/database/config/database.config.ts",
        "typeorm:generate": "npm run typeorm migration:generate UserEntity -- -d ./src/database/config/database.config.ts"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.9",
        "@types/pg": "^8.10.2",
        "@types/uuid": "^9.0.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "cpf-cnpj-validator": "^1.0.3",
        "dotenv": "^16.1.4",
        "express": "^4.18.2",
        "ioredis": "^5.3.2",
        "pg": "^8.11.1",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.11",
        "uuid": "^9.0.0"
    }
}
